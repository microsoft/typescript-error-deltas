{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES6",
        "composite": true,
        "strict": true,
        "skipLibCheck": true,
        "declaration": true,
        "sourceMap": true,
        "useUnknownInCatchVariables": false,
    },
    "include": [
        "*.ts", "github-url/*.ts"
    ],
    "exclude": [
        "userTests", "typescript-*"
    ]
}
